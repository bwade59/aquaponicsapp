<?xml version='1.0' encoding='utf-8'?>
<widget id="org.diamondtech.aquadome" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Aquadome</name>
    <description>
        Controller for Papa's igloo.
    </description>
    <author email="bwade1@nc.rr.com" href="http://www.diamondtech.org">
        Diamond Technical Consulting and Services LLC
    </author>.or
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <engine name="browser" spec="^5.0.1" />
    <engine name="android" spec="^6.3.0" />
    <engine name="ios" spec="^4.5.2" />
    <plugin name="cordova-plugin-device" spec="~1.1.6" />
    <plugin name="cordova-plugin-network-information" spec="~1.3.3" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
</widget>
